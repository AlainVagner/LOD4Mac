<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>LOD4Mac</title>
    <pkg-ref id="org.sous-anneau.lod4mac"/>
    <welcome file="welcome.html" mime-type="text/html"/>
    <conclusion file="conclusion.html" mime-type="text/html"/>
    <license file="LICENSE.txt"/>
    <options customize="never" require-scripts="false" hostArchitectures="x86_64,arm64"/>
    <domains enable_localSystem="true" />
    <choices-outline>
        <line choice="default">
            <line choice="org.sous-anneau.lod4mac"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="org.sous-anneau.lod4mac" visible="false" customLocation="/Library">
        <pkg-ref id="org.sous-anneau.lod4mac"/>
    </choice>
    <pkg-ref id="org.sous-anneau.lod4mac" onConclusion="none">lod.pkg</pkg-ref>
</installer-gui-script>
